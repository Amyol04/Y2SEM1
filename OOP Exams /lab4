public class Breed {
    private String name;
    private boolean endangeredList;

    // Default constructor
    public Breed() {
        this.name = "";
        this.endangeredList = false;
    }

    // Constructor with name
    public Breed(String name) {
        this.name = name;
        this.endangeredList = false;
    }

    // Constructor with name and endangered status
    public Breed(String name, boolean endangeredList) {
        this.name = name;
        this.endangeredList = endangeredList;
    }

    // Setters and Getters
    public void setBreedName(String name) {
        this.name = name;
    }

    public String getBreedName() {
        return name;
    }

    public void setEndangeredList(boolean endangeredList) {
        this.endangeredList = endangeredList;
    }

    public boolean getEndangeredList() {
        return endangeredList;
    }

    // toString method
    @Override
    public String toString() {
        return "Breed: " + name + ", Endangered: " + endangeredList;
    }

    // Print method
    public void print() {
        System.out.println(this.toString());
    }
}
